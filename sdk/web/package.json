{
  "name": "@nava/sdk-web",
  "version": "0.1.0",
  "description": "NAVΛ SDK for Web Platform - TypeScript/JavaScript bindings",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "browser": "dist/nava-sdk.js",
  "files": [
    "dist",
    "README.md",
    "assets"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:ts",
    "build:wasm": "wasm-pack build ../native --target web --out-dir pkg",
    "build:ts": "tsc && rollup -c",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nava",
    "sdk",
    "navigation",
    "robotics",
    "webassembly",
    "typescript"
  ],
  "author": "NAVΛ Team <team@nava.studio>",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nava-studio/nava-sdk.git"
  },
  "dependencies": {
    "@nava/sdk-wasm": "file:../native/pkg"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-node-resolve": "^15.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "wasm-pack": "^0.12.0"
  }
}

