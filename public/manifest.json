{
  "name": "NAVΛ Studio IDE - Enterprise Development Platform",
  "short_name": "NAVΛ Studio",
  "description": "Enterprise-grade IDE for Van Laarhoven Navigation Calculus with full offline support, 3D/7D navigation, WebRTC, and complete development environment",
  "start_url": "/workspace.html",
  "scope": "/",
  "display": "standalone",
  "orientation": "any",
  "background_color": "#0a1530",
  "theme_color": "#00ff00",
  "categories": ["development", "productivity", "education", "navigation", "utilities"],
  "lang": "en-US",
  "dir": "ltr",
  "prefer_related_applications": false,
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "icons": [
    {
      "src": "/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/favicon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/ide-view.png",
      "sizes": "1280x720",
      "type": "image/png",
      "platform": "wide",
      "label": "NAVΛ Studio IDE - Monaco Editor with Navigation Calculus"
    },
    {
      "src": "/screenshots/terminal-view.png",
      "sizes": "1280x720",
      "type": "image/png",
      "platform": "wide",
      "label": "Integrated Terminal with NAVΛ Commands"
    },
    {
      "src": "/screenshots/workspace-view.png",
      "sizes": "1280x720",
      "type": "image/png",
      "platform": "wide",
      "label": "NAVΛ OS Desktop Workspace"
    }
  ],
  "shortcuts": [
    {
      "name": "Open IDE",
      "short_name": "IDE",
      "description": "Open NAVΛ Studio IDE",
      "url": "/app.html?activity=explorer",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "Workspace",
      "short_name": "Workspace",
      "description": "Open Navigation Workspace",
      "url": "/workspace.html",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "New File",
      "short_name": "New",
      "description": "Create a new NAVΛ file",
      "url": "/app.html?activity=explorer&action=new",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "Terminal",
      "short_name": "Terminal",
      "description": "Open integrated terminal",
      "url": "/app.html?activity=explorer&panel=terminal",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    }
  ],
  "share_target": {
    "action": "/share-target/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": [
            "text/*",
            "application/*",
            ".nav",
            ".navλ",
            ".py",
            ".js",
            ".ts",
            ".rs",
            ".json",
            ".md",
            ".csv",
            ".txt"
          ]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/app.html?file=%s",
      "accept": {
        "text/navlambda": [".nav", ".navλ"],
        "text/plain": [".txt"],
        "application/json": [".json"],
        "text/markdown": [".md"],
        "application/javascript": [".js", ".mjs"],
        "application/typescript": [".ts"],
        "text/x-python": [".py"],
        "text/x-rust": [".rs"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+navlambda",
      "url": "/handler?url=%s"
    },
    {
      "protocol": "navlambda",
      "url": "/handler?url=%s"
    }
  ],
  "related_applications": [],
  "iarc_rating_id": "",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred"
}
